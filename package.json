{
  "name": "spider-mind-v2",
  "version": "1.3.4",
  "private": true,
  "description": "Spider Mind v2 - 智能知识管理和思维导图应用",
  "author": "Colin <biz@colinhan.info>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:turbo": "node scripts/dev.js --turbo",
    "dev:kill": "node scripts/kill-dev-server.js",
    "dev:kill:force": "node scripts/kill-dev-server.js --force",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "export": "next export",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=tests/e2e",
    "test:clear-cache": "jest --clearCache",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:codegen": "playwright codegen",
    "test:all": "yarn test:unit && yarn test:e2e",
    "validate": "yarn lint && yarn type-check && yarn test:unit",
    "validate:full": "yarn validate && yarn test:e2e",
    "clean": "rm -rf .next out coverage test-results playwright-report node_modules/.cache",
    "clean:deps": "rm -rf node_modules yarn.lock && yarn install",
    "prepare": "husky install",
    "postinstall": "yarn test:e2e:install",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:check": "node scripts/check-env.js",
    "db:types": "mkdir -p src/lib/types && (supabase gen types typescript --local > src/lib/types/supabase.ts.tmp 2>/dev/null && mv src/lib/types/supabase.ts.tmp src/lib/types/supabase.ts || echo 'Supabase项目未初始化，保留当前类型定义')",
    "db:reset": "supabase db reset",
    "db:seed": "npx tsx scripts/seed-test-users.ts",
    "db:push": "./scripts/db-push.sh",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "feat:new": "./scripts/new-feature.sh",
    "feat:state": "./scripts/feature-manager.sh"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.41",
    "@ai-sdk/deepseek": "^1.0.27",
    "@ai-sdk/openai": "^2.0.64",
    "@ai-sdk/react": "^2.0.89",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.80.0",
    "@uiw/react-md-editor": "^4.0.8",
    "@xyflow/react": "^12.8.6",
    "ai": "^5.0.89",
    "clsx": "^2.1.1",
    "dagre": "^0.8.5",
    "date-fns": "^4.1.0",
    "idb": "^8.0.3",
    "immer": "^10.1.3",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.545.0",
    "nanoid": "^5.1.6",
    "next": "^15",
    "react": "^18.3.0",
    "react-arborist": "^3.4.3",
    "react-dom": "^18.3.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dagre": "^0.7.53",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "supabase": "^2.58.5",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@1.22.22",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "bash -c 'tsc --project tsconfig.json --noEmit'"
    ],
    "*.{js,jsx,json,md,css}": [
      "prettier --write"
    ]
  },
  "volta": {
    "yarn": "1.22.22",
    "node": "24.2.0"
  }
}
